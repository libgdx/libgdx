<?xml version="1.0" encoding="UTF-8"?>
<extensions>
	<extension>
		<name>spine-libgdx</name>
		<description>Spine Runtimes for loading and rendering 2D skeletal animation</description>
		<package>com.esotericsoftware.spine</package>
		<version>3.8.55.1</version>
		<compatibility>1.9.10</compatibility>
		<website>http://esotericsoftware.com/</website>
		<gwtInherits></gwtInherits>
		<projects>
			<core>
				<dependency>com.esotericsoftware.spine:spine-libgdx</dependency>
			</core>
			<desktop></desktop>
			<android></android>
			<ios></ios>
			<html></html>
		</projects>
	</extension>
	<extension>
		<name>VisUI</name>
		<description>Flat design skin for scene2d.ui and UI toolkit</description>
		<package>com.kotcrab.vis.ui</package>
		<version>1.4.11</version>
		<compatibility>1.9.14</compatibility>
		<website>https://github.com/kotcrab/VisEditor/wiki/VisUI</website>
		<gwtInherits>
			<inherit>com.kotcrab.vis.vis-ui</inherit>
		</gwtInherits>
		<projects>
			<core>
				<dependency>com.kotcrab.vis:vis-ui</dependency>
			</core>
			<desktop></desktop>
			<android></android>
			<ios></ios>
			<html>
				<dependency>com.kotcrab.vis:vis-ui:sources</dependency>
			</html>
		</projects>
	</extension>
	<extension>
		<name>jbump</name>
		<description>A 2D AABB collision detection and response library</description>
		<package>com.dongbat.jbump</package>
		<version>1.0.1</version>
		<compatibility>1.9.13</compatibility>
		<website>https://github.com/tommyettinger/jbump</website>
		<gwtInherits>
			<inherit>com.dongbat.jbump</inherit>
		</gwtInherits>
		<projects>
			<core>
				<dependency>com.github.tommyettinger:jbump</dependency>
			</core>
			<desktop></desktop>
			<android></android>
			<ios></ios>
			<html>
				<dependency>com.github.tommyettinger:jbump:sources</dependency>
			</html>
		</projects>
	</extension>
	<extension>
		<name>gdx-gamesvcs</name>
		<description>Provides support for Google Play Games, Apple Game Center, GameJolt API</description>
		<package>de.golfgl.gdxgamesvcs</package>
		<version>1.1.0</version>
		<compatibility>1.9.8</compatibility>
		<website>https://github.com/MrStahlfelge/gdx-gamesvcs</website>
		<gwtInherits>
			<inherit>de.golfgl.gdxgamesvcs.gdx_gamesvcs_gwt</inherit>
		</gwtInherits>
		<projects>
			<core>
				<dependency>de.golfgl.gdxgamesvcs:gdx-gamesvcs-core</dependency>
			</core>
			<desktop>
				<dependency>de.golfgl.gdxgamesvcs:gdx-gamesvcs-core-gamejolt</dependency>
			</desktop>
			<android>
				<dependency>de.golfgl.gdxgamesvcs:gdx-gamesvcs-android-gpgs</dependency>
			</android>
			<ios>
				<dependency>de.golfgl.gdxgamesvcs:gdx-gamesvcs-ios-gamecenter</dependency>
			</ios>
			<html>
				<dependency>de.golfgl.gdxgamesvcs:gdx-gamesvcs-core:sources</dependency>
				<dependency>de.golfgl.gdxgamesvcs:gdx-gamesvcs-core-gamejolt:sources</dependency>
			</html>
		</projects>
	</extension>
	<extension>
	   <name>gdx-dialogs</name>
	   <description>Provides cross-platform support for native dialogs</description>
	   <package>de.tomgrill.gdxdialogs</package>
	   <version>1.3.0</version>
	   <compatibility>1.9.6</compatibility>
	   <website>https://github.com/TomGrill/gdx-dialogs</website>
	   <gwtInherits></gwtInherits>
	   <projects>
			<core>
				<dependency>de.tomgrill.gdxdialogs:gdx-dialogs-core</dependency>
			</core>
			<desktop>
				<dependency>de.tomgrill.gdxdialogs:gdx-dialogs-desktop</dependency>
			</desktop>
			<android>
				<dependency>de.tomgrill.gdxdialogs:gdx-dialogs-android</dependency>
			</android>
			<ios>
				<dependency>de.tomgrill.gdxdialogs:gdx-dialogs-ios</dependency>
			</ios>
			<html>
		   		<dependency>de.tomgrill.gdxdialogs:gdx-dialogs-html</dependency>
				<dependency>de.tomgrill.gdxdialogs:gdx-dialogs-html:sources</dependency>
				<dependency>de.tomgrill.gdxdialogs:gdx-dialogs-core:sources</dependency>
		   	</html>
		</projects>
	</extension>
	<extension>
		<name>gdx-websockets</name>
		<description>Provides client-side web sockets support</description>
		<package>com.github.czyzby.websocket</package>
		<version>1.1.0</version>
		<compatibility>1.9.10</compatibility>
		<website>https://github.com/MrStahlfelge/gdx-websockets</website>
		<gwtInherits>
			<inherit>com.github.czyzby.websocket.GdxWebSocket</inherit>
			<inherit>com.github.czyzby.websocket.GdxWebSocketGwt</inherit>
		</gwtInherits>
		<projects>
			<core>
				<dependency>com.github.MrStahlfelge.gdx-websockets:core</dependency>
			</core>
			<desktop>
				<dependency>com.github.MrStahlfelge.gdx-websockets:common</dependency>
			</desktop>
			<android>
				<dependency>com.github.MrStahlfelge.gdx-websockets:common</dependency>
			</android>
			<ios>
				<dependency>com.github.MrStahlfelge.gdx-websockets:common</dependency>
			</ios>
			<html>
				<dependency>com.github.MrStahlfelge.gdx-websockets:core:sources</dependency>
				<dependency>com.github.MrStahlfelge.gdx-websockets:html</dependency>
				<dependency>com.github.MrStahlfelge.gdx-websockets:html:sources</dependency>
			</html>
		</projects>
	</extension>
</extensions>
