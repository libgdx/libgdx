	<widgetStyles>
		<button name="default" down="default-round-down" up="default-round" font="default-font" fontColor="white"/>		
		<imagebutton name="default" down="default-round-down" up="default-round"/>
		<togglebutton name="default" down="default-round-down" up="default-round" font="default-font" fontColor="white"/>
		<checkbox name="default" font="default-font" fontColor="white" checked="check-on" unchecked="check-off"/>
		<label name="default" font="default-font" fontColor="white"/>
		<slider name="default" slider="default-slider" knob="default-slider-knob"/>
		<list name="default" font="default-font" fontColorUnselected="white" fontColorSelected="white" selected="default-rect-pad"/>
		<pane name="default" background="default-round"/>
		<pane name="default-rect" background="default-rect"/>
		<pane name="noborder" background="default-pane-noborder"/>
		<scrollpane name="default" background="default-rect" hScroll="default-slider" hScrollKnob="default-slider-knob" vScroll="default-slider" vScrollKnob="default-slider-knob"/>
		<splitpane name="default-horizontal" handle="default-splitpane"/>
		<splitpane name="default-vertical" handle="default-splitpane-vertical"/>
		<textfield name="default" font="default-font" fontColor="white" cursor="cursor" selection="selection" background="textfield"/>
		<combobox name="default" font="default-font" fontColor="white" background="default-combo" listBackground="default-rect" listSelection="default-combo-selection"/>
